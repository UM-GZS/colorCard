<view><view class="swiper-box"><swiper class="swiper-list" circular="{{true}}" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" indicator-active-color="#fff"><swiper-item><image class="swiper-item" src="http://rf7ocvhsf.hn-bkt.clouddn.com/colorCard/prod/banner.png"></image></swiper-item></swiper></view><view class="tool-list"><block wx:for="{{toolList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toUrl',['$0','$1'],[[['toolList','',index,'page']],[['toolList','',index,'isTab']]]]]]]}}" class="tool-item" bindtap="__e"><view class="item-left"><view class="item-title">{{item.name}}</view><view class="item-info">{{item.info}}</view><view class="item-go">立即体验</view></view><view class="item-right"><image src="{{item.url}}"></image></view></view></block></view></view>