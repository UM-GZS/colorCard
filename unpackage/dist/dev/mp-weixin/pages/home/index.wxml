<view><view class="nav-list"><scroll-view class="nav-scroll" scroll-x="{{true}}"><block wx:for="{{navList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['selectNav',[index]]]]]}}" class="{{['nav-item',(navIndex===index)?'active':'']}}" bindtap="__e"><view class="nav-name">{{item.name}}</view><view class="nav-line"></view></view></block></scroll-view></view><block wx:if="{{navIndex===0}}"><block wx:for="{{itemList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="color-card"><view class="card-head">{{item.name}}</view><view class="card-list"><block wx:for="{{item.child}}" wx:for-item="ite" wx:for-index="indx" wx:key="indx"><view class="card-item"><image src="{{ite.image}}" mode="center" data-event-opts="{{[['tap',[['previewImg',['$0'],[[['itemList','',index],['child','',indx,'image']]]]]]]}}" bindtap="__e"></image><view class="card-tag">{{ite.name}}</view></view></block></view></view></block></block><block wx:else><view class="color-list"><block wx:for="{{cardList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="color-item"><image src="{{item.image}}" mode="center" data-event-opts="{{[['tap',[['previewImg',['$0'],[[['cardList','',index,'image']]]]]]]}}" bindtap="__e"></image><view class="color-name">{{item.name}}</view></view></block></view></block><view class="page-more">没有更多内容了</view></view>